@model List<EMS_App.Models.Event>

<link href="~/css/event.css" rel="stylesheet" asp-append-version="true" />
<h3 class="page-title text-center">Timeline</h3>
<ul class="timeline">
    <li class="timeline-line"></li>
    <li class="timeline-group">
        <a href="#" class="btn btn-primary"><i class="fa fa-calendar"></i>December 6th, 2023</a>
    </li>
</ul>
<ul class="timeline">
    <li class="timeline-line"></li>
    @foreach (Event e in Model)
    {
        <li class="timeline-item">
            <div class="timeline-badge"><a href="#"></a></div>
            <div class="timeline-panel">
                <div class="timeline-heading">
                    @e.eventHeader
                    <div class="timeline-date"><i class="fa fa-clock-o"></i> @e.eventStartTime.ToString("MMMM dd, yyyy - HH:mm")</div>
                </div>
                <div class="timeline-content">
                    <p>@Html.Raw(e.eventShortDescription)</p>
                </div>
                <div class="timeline-footer">
                    <small class="text-muted">Duration = @(e.eventEndTime - e.eventStartTime)</small>
                </div>
            </div>
        </li>
    }
